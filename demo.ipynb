   "source": [
    "from pySlope import Slope, Material\n",
    "\n",
    "s = Slope(height=5, angle=None, length=8)\n",
    "\n",
    "sand = Material(20,30,0,1)\n",
    "clay = Material(18,25,5,3)\n",
    "\n",
    "s.add_materials(sand,clay)\n",
    "\n",
    "\n",
    "fig = s.plot_boundary()\n",
    "fig.show()\n"
   ]
  }
